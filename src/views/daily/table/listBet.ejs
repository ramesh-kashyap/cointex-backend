  <script>
    $(document).ready(function() {
        let currentPage = 1;
        const limit = 5;
    
        // Function to fetch data from the API
        function fetchBets(page = 1) {
            $.ajax({
                url: `/api/webapi/WingoBetList?page=${page}&limit=${limit}`, // Replace with your actual API endpoint
                method: 'POST',
                success: function(response) {
                    if (response.status) {
                        console.log(response.data.gameslist);
                        renderBets(response.data.gameslist);
                        updatePagination(response.data.currentPage, response.data.totalPages);
                    } else {
                        console.error('Failed to fetch data:', response.message);
                    }
                },
                error: function(error) {
                    console.error('Error fetching data:', error);
                }
            });
        }
    
        // Function to render bets
        function renderBets(bets) {
            const container = $('#bet-container-items');
            container.empty(); // Clear existing content
    
            bets.forEach(bet => {
                const betHtml = `
                    <div data-v-4f01bad5="" class="bet-container-lottery">
                        <div data-v-4f01bad5="" class="bet-container-lottery-items">
                            <div data-v-4f01bad5="" class="bet-container-lottery-card">
                                <div data-v-4f01bad5="" class="bet-container-lottery-card-header ar-1px-b">
                                    <h1 data-v-4f01bad5="">
                                        <h2 data-v-4f01bad5="">${bet.game == "wingo10"
                                                ? "Wingo 10 minutes"
                                                : bet.game == "wingo5"
                                                ? "Wingo 5 minutes"
                                                : bet.game == "wingo3"
                                                ? "Wingo 3 minutes"
                                                : "Wingo 1 minutes"}</h2>
                                        <span data-v-4f01bad5="" class="${bet.status === 1 ? 'color40C592' : 'colorE98613'}">${bet.status === 1 ? 'Win' : 'Lose'}</span>
                                    </h1>
                                    <p data-v-4f01bad5="">${bet.today}</p>
                                </div>
                                <div data-v-4f01bad5="" class="bet-container-lottery-card-info">
                                    <img data-v-4f01bad5="" src="/images/orders1.png">
                                    <ul data-v-4f01bad5="">
                                        <li data-v-4f01bad5="">
                                            <h2 data-v-4f01bad5="">Type</h2><span data-v-4f01bad5="">${bet.game}</span>
                                        </li>
                                        <li data-v-4f01bad5="">
                                            <h2 data-v-4f01bad5="">Period</h2><span data-v-4f01bad5="">${bet.stage}</span>
                                        </li>
                                        <li data-v-4f01bad5="">
                                            <h2 data-v-4f01bad5="">Order number</h2><span data-v-4f01bad5="">${bet.id_product}</span>
                                        </li>
                                        <li data-v-4f01bad5="">
                                            <h2 data-v-4f01bad5="">Select</h2><p data-v-4f01bad5="">${  bet.result == 0
                                                ? "Red purple"
                                                : bet.result == 5
                                                ? "Purple green"
                                                : bet.result % 2 == 0
                                                ? "Red"
                                                : "Green"}</p>
                                        </li>
                                        <li data-v-4f01bad5="">
                                            <h2 data-v-4f01bad5="">Total bet</h2><span data-v-4f01bad5="">₹${bet.money + bet.fee}</span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <img data-v-4f01bad5="" src="https://bdggame.org/assets/png/moonBar-31e52056.png">
                            <div data-v-4f01bad5="" class="bet-container-lottery-note">
                                <div data-v-4f01bad5="" class="bet-container-lottery-note-result">
                                    <img data-v-4f01bad5="" src="/images/orders2.png">
                                    <div>
                                        <h1 data-v-4f01bad5="">Lottery results</h1>
                                        <h2 data-v-4f01bad5="">
                                            <p data-v-4f01bad5="">${bet.result}</p>
                                            <span data-v-4f01bad5="" class="${bet.result  > 4 ? 'bigClass' : 'smallClass'}">${bet.result > 4 ? 'Big' : 'Small'}</span>
                                            <span data-v-4f01bad5="" class="${bet.get > 0 ? 'green' : 'red'}">${ bet.result == 0
                                                ? "Red purple"
                                                : bet.result == 5
                                                ? "Purple green"
                                                : bet.result % 2 == 0
                                                ? "Red"
                                                : "Green"}</span>
                                        </h2>
                                    </div>
                                </div>
                                <div data-v-4f01bad5="" class="bet-container-lottery-note-box">
                                    <div data-v-4f01bad5="">
                                        <div data-v-4f01bad5="" class="bet-container-lottery-note-box-para">
                                            <h3 data-v-4f01bad5="">₹${bet.money}</h3><span data-v-4f01bad5="">Actual amount</span>
                                        </div>
                                    </div>
                                    <div data-v-4f01bad5="">
                                        <div data-v-4f01bad5="" class="bet-container-lottery-note-box-para">
                                            <h3 data-v-4f01bad5="">₹${bet.fee}</h3><span data-v-4f01bad5="">Handling fee</span>
                                        </div>
                                    </div>
                                    <div data-v-4f01bad5="">
                                        <div data-v-4f01bad5="" class="bet-container-lottery-note-box-para">
                                            <h3 data-v-4f01bad5="">₹${bet.get}</h3><span data-v-4f01bad5="">Winnings</span>
                                        </div>
                                    </div>
                                    <div data-v-4f01bad5="">
                                        <div data-v-4f01bad5="" class="bet-container-lottery-note-box-para">
                                            <h4 data-v-4f01bad5="" class="${bet.result === 1 ? 'h4_green' : 'h4_red'}">₹${-bet.money + bet.get - bet.fee }</h4><span data-v-4f01bad5="">Profit/loss</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                `;
                container.append(betHtml);
            });
        }
    
        // Function to update pagination controls
        function updatePagination(currentPage, totalPages) {
            $('#page-info').text(`${currentPage}/${totalPages}`);
            $('#prev-page').toggleClass('disabled', currentPage === 1);
            $('#next-page').toggleClass('disabled', currentPage === totalPages);
        }
    
        // Initial fetch
        fetchBets(currentPage);
    
        // Pagination event listeners
        $('#prev-page').click(function() {
            if (!$(this).hasClass('disabled')) {
                currentPage--;
                fetchBets(currentPage);
            }
        });
    
        $('#next-page').click(function() {
            if (!$(this).hasClass('disabled')) {
                currentPage++;
                fetchBets(currentPage);
            }
        });
    });
    </script>
    
 
