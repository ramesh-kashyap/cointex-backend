        </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="/js/client.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
   $(document).ready(function() {
    // Toggle the display of the dropdown menu
    $('[data-v-f330f1f2]').click(function() {
        $('.van-overlay, .van-popup').toggle();
    });

    // Hide the dropdown menu when the cancel button is clicked
    $('.van-picker__cancel').click(function() {
        $('.van-overlay, .van-popup').hide();
    });

    // Function to format time
    function formateT(params) {
        let result = (params < 10) ? "0" + params : params;
        return result;
    }

    // Function to join timer
    function timerJoin(params = '') {
        let date = '';
        if (params) {
            date = new Date(params);
        } else {
            date = new Date();
        }
        let years = formateT(date.getFullYear());
        let months = formateT(date.getMonth() + 1);
        let days = formateT(date.getDate());

        return years + '-' + months + '-' + days;
    }

    // Set initial state of navigation items
    $('.nav .van-tabbar-item:eq(0) img').attr('src', '/images/home1.png');
    $('.nav .van-tabbar-item:eq(0) .name').removeClass('action');
    $('.nav .van-tabbar-item:eq(2) .name').addClass('action');

    // Function to render member data
    function MemRender(datas) {
        let html = '';
        datas.map((data) => {
            html += `
                <div data-v-cbab7763="" data-v-e4760c44="" class="infiniteScroll" id="refresh5fb40baadfa743dbbfe286bd2ff24447">
                    <div data-v-e4760c44="" class="rechargeh__container-content__item">
                        <div data-v-e4760c44="" class="rechargeh__container-content__item-header ar-1px-b">
                            <span data-v-e4760c44="">${data.remarks}</span>
                            <span data-v-e4760c44="" class="stateG"></span>
                        </div>
                        <div data-v-e4760c44="" class="rechargeh__container-content__item-body">
                            <div data-v-e4760c44=""><span data-v-e4760c44="">Balance</span><span
                                    data-v-e4760c44="">â‚¹${data.comm}</span></div>
                            <div data-v-e4760c44=""><span data-v-e4760c44="">Time</span><span
                                    data-v-e4760c44="">${timerJoin(data.updated_at)}</span></div>
                            <div data-v-e4760c44=""><span data-v-e4760c44="">Remark</span><span
                                    data-v-e4760c44="">${data.remarks}</span></div>
                            ${data.remarks === 'Team Comission Bonus' ? `
                            <div data-v-e4760c44=""><span data-v-e4760c44="">Level</span><span
                                    data-v-e4760c44="">${data.bet ? data.bet : 0}</span></div>
                            ` : ''}
                            ${data.remarks === 'AI Return Bonus' ? `
                            <div data-v-e4760c44=""><span data-v-e4760c44="">Level</span><span
                                    data-v-e4760c44="">${data.bet ? data.bet : 0}</span></div>
                            ` : ''}
                        </div>
                    </div>
                </div>
                <br>
            `;
        });
        $('#van-list').html(html);
    }

    // Function to display alert messages
    function alertMess(text, sic) {
        $('body').append(`
            <div data-v-1dcba851="" class="msg">
                <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
            </div>
        `);
        setTimeout(() => {
            $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
            $('.msg .msg-content').addClass('v-leave-active v-leave-to');
            setTimeout(() => {
                $('.msg').remove();
            }, 100);
            sic.removeClass('block-click');
        }, 1000);
    }

    let currentPage = 1;
    const limit = 5;
    let filterOption = 'All';

    function fetchIncomeReports(page = 1) {
        $.ajax({
            type: "GET",
            url: `/api/webapi/listIncomeReport?page=${page}&limit=${limit}`,
            dataType: "json",
            success: function(response) {
                if (response.incomeReports.length == 0) {
                    $('#van-list').html('<p>No data available for the selected filter.</p>');
                    return;
                }

                let filteredReports = response.incomeReports;
                if (filterOption !== 'All') {
                    filteredReports = response.incomeReports.filter(report => report.remarks === filterOption);
                }

                if (filteredReports.length == 0) {
                    $('#van-list').html('<p>No data available for the selected filter.</p>');
                    return;
                }

                MemRender(filteredReports);
                updatePagination(response.currentPage, response.totalPages);
            },
            error: function(error) {
                console.error('Error fetching data:', error);
            }
        });
    }

    function updatePagination(currentPage, totalPages) {
        $('#page-info').text(`${currentPage}/${totalPages}`);
        $('#prev-page').toggleClass('disabled', currentPage === 1);
        $('#next-page').toggleClass('disabled', currentPage === totalPages);
    }

    // Initial fetch
    fetchIncomeReports(currentPage);

    // Pagination event listeners
    $('#prev-page').click(function() {
        if (!$(this).hasClass('disabled')) {
            currentPage--;
            fetchIncomeReports(currentPage);
        }
    });

    $('#next-page').click(function() {
        if (!$(this).hasClass('disabled')) {
            currentPage++;
            fetchIncomeReports(currentPage);
        }
    });

    // Filter event listener
    $('.van-picker-column__item').click(function() {
        filterOption = $(this).find('.van-ellipsis').text();
        currentPage = 1; // Reset to first page when filter changes
        fetchIncomeReports(currentPage);
        $('.van-popup, .van-overlay').hide();
        $('.ar-searchbar__selector-default').text(filterOption);
    });
});

    </script>
    


</body>

</html>