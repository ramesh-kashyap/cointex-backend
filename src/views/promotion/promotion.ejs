        $(document).ready(function () {
            // Function to show alert messages
            function alertMess(text, sic) {
                $('body').append(
                    `<div data-v-1dcba851="" class="msg">
                        <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to"> ${text} </div>
                    </div>`
                );
                setTimeout(() => {
                    $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                    $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                    setTimeout(() => {
                        $('.msg').remove();
                    }, 100);
                    sic.removeClass('block-click');
                }, 1000);
            }
    
            // Fetch and update data from the API
            function updateDirectSubordinates() {
                $.ajax({
                    url: '/api/webapi/directTeamDetails', // Update this URL to match your API endpoint
                    method: 'GET',
                    success: function (response) {
                        if (response.status) {
                            $('#numberOfRegister').text(response.directSubordinates.numberOfRegister);
                            $('#depositNumber').text(response.directSubordinates.depositNumber);
                            $('#depositAmount').text(response.directSubordinates.depositAmount);
                            $('#todayDepositAmount').text(response.directSubordinates.todayDepositAmount);
                            $('#firstDepositCount').text(response.directSubordinates.firstDepositCount);
                        } else {
                            alertMess('Failed to fetch data', $('body'));
                        }
                    },
                    error: function () {
                        alertMess('Error fetching data', $('body'));
                    }
                });
            }
    
            // Initial data fetch
            updateDirectSubordinates();

             // Fetch and update data from the API for team subordinates
        function updateTeamSubordinates() {
            $.ajax({
                url: '/api/webapi/teamSubordinatesDetails', // Update this URL to match your API endpoint
