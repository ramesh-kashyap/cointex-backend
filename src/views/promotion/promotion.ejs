                        $('#teamDepositNumber').text(response.directSubordinates.totalDepositNumber);
                        $('#teamDepositAmount').text(response.directSubordinates.totalDepositAmount);
                        $('#todayTeamDepositAmount').text(response.directSubordinates.todayDepositAmount);
                        $('#teamFirstDepositCount').text(response.directSubordinates.totalFirstDepositCount);
                    } else {
                        alertMess('Failed to fetch data', $('body'));
                    }
                },
                error: function () {
                    alertMess('Error fetching data', $('body'));
                }
            });
        }

        // Initial data fetch for team subordinates
